    "electron": "^1.4.3",
    "electron-builder": "^20.15.1",
    "electron-context-menu": "^0.10.0",
    "electron-media-service": "^0.2.2",
    "electron-window-manager": "^1.0.6",
    "webpack-cli": "^2.0.15"
  }
